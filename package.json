{
  "name": "ikeacn-comparison",
  "version": "1.0.0",
  "description": "the comparison application based on vue3 + typescript",
  "main": "index.js",
  "keywords": [
    "vue",
    "typescript",
    "ddd",
    "spa",
    "ikeacn",
    "comparison"
  ],
  "scripts": {
    "prepare": "husky install",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "dev": "cp env/env.dev .env && vite --mode development --host --port 4001",
    "build:dev": "npm run clean:dist && cp env/env.dev .env && vite build --mode production",
    "build:qa": "npm run clean:dist && cp env/env.qa .env && vite build --mode production",
    "build:uat": "npm run clean:dist && cp env/env.uat .env && vite build --mode production",
    "build:ppe": "npm run clean:dist && cp env/env.ppe .env && vite build --mode production",
    "build:prod": "npm run clean:dist && cp env/env.prod .env && vite build --mode production",
    "build:report": "npm run clean:dist && cp env/env.prod .env && vite build --mode report",
    "serve": "vite preview",
    "stylelint": "stylelint --config stylelint.config.js src/**/*.{vue,scss}",
    "stylelint:fix": "stylelint --config stylelint.config.js --fix src/**/*.{vue,scss}",
    "linter": "eslint -c .eslintrc.js --ext .js,.vue,.ts, src",
    "linter:fix": "eslint -c .eslintrc.js --fix --ext .js,.vue,.ts, src",
    "lint": "npm run linter && npm run stylelint",
    "lint:fix": "npm run linter:fix && npm run stylelint:fix",
    "test": "npm run clean:coverage && jest",
    "test:watch": "npm run clean:coverage && jest --watch"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/vue": "^6.4.2",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vitejs/plugin-legacy": "^1.3.4",
    "@vue/compiler-sfc": "^3.2.13",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.9.0",
    "jest": "26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.6.4",
    "shelljs": "^0.8.4",
    "standard": "^16.0.3",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-rscss": "^0.4.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "26.5.6",
    "vue-jest": "^5.0.0-alpha.10",
    "sass": "^1.32.6",
    "sass-loader": "^12.1.0"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.0.7",
    "@iconify/json": "^1.1.343",
    "@ikeacn/adapter": "^1.2.29",
    "@ikeacn/track": "^1.0.43",
    "@ingka/button": "^6.0.3",
    "@ingka/focus": "^3.0.8",
    "@ingka/forms": "^4.0.16",
    "@ingka/hyperlink": "^4.0.8",
    "@ingka/inline-text": "^2.1.1",
    "@ingka/loading": "^4.0.8",
    "@ingka/modal": "^0.9.11",
    "@ingka/toast": "^6.0.11",
    "@intlify/vite-plugin-vue-i18n": "^2.1.2",
    "@rollup/plugin-yaml": "^3.0.0",
    "@turbo/eslint-config": "^1.0.0",
    "@turboui/icons": "^1.0.18",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vuedx/typecheck": "^0.6.3",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "@vuelidate/core": "^2.0.0-alpha.18",
    "@vuelidate/validators": "^2.0.0-alpha.15",
    "@vueuse/integrations": "^4.11.0",
    "ant-design-vue": "^2.2.2",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "camelcase": "^6.2.0",
    "dotenv-cli": "^4.0.0",
    "focus-trap": "^6.7.1",
    "focus-trap-vue": "^3.2.1",
    "husky": "^7.0.0",
    "js-cookie": "^2.2.1",
    "postcss": "^8.2.15",
    "postcss-import": "^14.0.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.5.0",
    "swiper": "^7.3.1",
    "tailwindcss": "^2.2.4",
    "throttle-debounce": "^3.0.1",
    "typescript": "^4.2.4",
    "vite": "^2.6.13",
    "vite-plugin-components": "^0.13.2",
    "vite-plugin-fonts": "^0.2.2",
    "vite-plugin-icons": "^0.5.1",
    "vite-plugin-style-import": "^1.1.1",
    "vue": "^3.2.21",
    "vue-i18n": "^9.1.6",
    "vue-inline-svg": "^2.0.0",
    "vue-router": "^4.0.8",
    "vue3-lazy": "^1.0.0-alpha.1",
    "vuex": "^4.0.0"
  },
  "volta": {
    "node": "14.17.1",
    "yarn": "1.22.17"
  }
}
